<?xml version='1.0' encoding='utf-8'?>
<widget id="atalis.cordova.tests" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>PluginTests</name>
    <content src="index.html" />
    <access origin="*" />
    
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    
    <preference name="SplashScreen" value="splash_bm"/>
<!--     <preference name="SplashScreen" value="splash_layer_list"/> -->
<!--     <preference name="SplashScreen" value="splash_animation_list"/> -->
    <preference name="SplashScreenDelay" value="60000" />
    <preference name="SplashMaintainAspectRatio" value="true"/>
</widget>

